package week03;

import java.util.Scanner;

public class RectangleTest {
	public static void main(String[] args) {
		int x1, y1, x2, y2;
		
		Scanner sc = new Scanner(System.in);
		x1 = sc.nextInt();
		y1 = sc.nextInt();
		while(true) {
			x2 = sc.nextInt();
			if(x2 <= x1) {
				System.out.println("좌측 꼭짓점보다 숫자가 작습니다. 다시 입력하세요.");
				continue;			}
			else
				break;
			}
		while(true) {
			y2 = sc.nextInt();
			if(x2 <= x1) {
				System.out.println("좌측 꼭짓점보다 숫자가 작습니다. 다시 입력하세요.");
				continue;
			}
			else
				break;
		}
	}
}
